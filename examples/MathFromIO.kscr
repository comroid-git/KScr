package org.comroid.kscr.test;

public class MathFromIO {
    public static void main() {
        stdio << "Operation: (A + B) * C"

        stdio << "\nEnter number A: "
        stdio >> num a;

        stdio << "\nEnter number B: "
        stdio >> num b;

        stdio << "\nEnter integer factor C: "
        stdio >> int c;

        stdio << "\nResult: "
        stdio << (a + b) * c;
    }
}
